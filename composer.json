{
    "name": "walnut/url-shortener",
    "autoload": {
        "psr-4": {
            "Walnut\\UrlShortener\\": "src/",
            "Walnut\\Test\\UrlShortener\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Marian Kostadinov",
            "email": "manchokapitancho@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "psr/http-message": ">=1.0.0",
        "psr/http-server-middleware": ">=1.0.2",
        "psr/http-server-handler": ">=1.0.1",
        "psr/http-factory": ">=1.1.0",
		"guzzlehttp/psr7": ">=2.7.0",
        "walnut/lib_container": "dev-main",
        "walnut/lib_data": "dev-main",
        "walnut/lib_http": "dev-main",
        "walnut/lib_db": "dev-main",
        "walnut/lib_identitygenerator": "^0.0.1"
    },
    "require-dev": {
        "vimeo/psalm": ">=5.2.0",
        "phpstan/phpstan": ">=1.9.2",
        "phpunit/phpunit": ">=9.5.27",
        "infection/infection": ">=0.26.16",
        "phpmetrics/phpmetrics": "^2.8"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": false
        }
    }
}
